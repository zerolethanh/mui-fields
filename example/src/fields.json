{
  "nameGender": {
    "grid": true,
    "direction": "row",
    "spacing": 1,
    "items": {
      "fullName": {
        "label": "Họ và tên",
        "value": "Lê Thành",
        "required": true,
        "autoFocus": true
      },
      "gender": {
        "radios": true,
        "title": "giới tính",
        "options": [
          {
            "value": "m",
            "label": "nam"
          },
          {
            "value": "f",
            "label": "nữ"
          }
        ]
      }
    }
  },
  "cmndDob": {
    "grid": true,
    "direction": "row",
    "spacing": 1,
    "items": {
      "cmnd": {
        "label": "Số cmnd/thẻ căn cước",
        "type": "number"
      },
      "dob": {
        "label": "Ngày sinh",
        "type": "date",
        "defaultValue": "2010-01-20"
      }
    }
  },
  "tinhSothichDev": {
    "grid": true,
    "direction": "row",
    "spacing": 1,
    "items": {
      "province": {
        "label": "Nơi ở hiện tại",
        "select": true,
        "items": [
          {
            "code": "HAN",
            "name": "Hà Nội"
          },
          {
            "code": "SGN",
            "name": "HCM"
          }
        ],
        "keyPath": "code",
        "valuePath": "code",
        "labelPath": "name",
        "style": {
          "width": 200
        }
      },
      "sothich": {
        "isMultipleSelect": true,
        "label": "Sở thích",
        "items": [
          "lập trình",
          "đọc sách báo",
          "chụp ảnh",
          "du lịch"
        ]
      },
      "dev": {
        "isMultipleSelect": true,
        "label": "Dev",
        "items": [
          {
            "value": "web",
            "label": "Web"
          },
          {
            "value": "mobile-app",
            "label": "Mobile App"
          },
          {
            "value": "AI",
            "label": "AI"
          },
          {
            "value": "block-chain",
            "label": "Block Chain"
          }
        ],
        "keyPath": "value",
        "valuePath": "value",
        "labelPath": "label"
      }
    }
  },
  "isVietNamese": {
    "label": "Là người Việt Nam",
    "checkbox": true,
    "checked": true,
    "fullWidth": true
  },
  "submit": {
    "type": "submit"
  }
}
